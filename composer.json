{
    "name": "sabitahmad/laravel-steadfast",
    "description": "Comprehensive Laravel package for SteadFast Courier API integration with bulk processing, return requests, caching, events, and advanced monitoring",
    "keywords": [
        "SabitAhmad",
        "laravel",
        "laravel-steadfast",
        "steadfast-courier",
        "courier-api",
        "bulk-orders",
        "return-requests",
        "order-tracking", 
        "delivery-api",
        "bangladesh-courier"
    ],
    "homepage": "https://github.com/sabitahmadumid/laravel-steadfast",
    "license": "MIT",
    "authors": [
        {
            "name": "Sabit Ahmad",
            "email": "sabitahmadumid@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0"
    },
    "autoload": {
        "psr-4": {
            "SabitAhmad\\SteadFast\\": "src/",
            "SabitAhmad\\SteadFast\\Database\\Factories\\": "database/factories/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SabitAhmad\\SteadFast\\SteadFastServiceProvider"
            ],
            "aliases": {
                "SteadFast": "SabitAhmad\\SteadFast\\Facades\\SteadFast"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
